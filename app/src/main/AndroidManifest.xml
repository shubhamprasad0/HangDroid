<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shubham.hangDroid">

    <application
        android:allowBackup="true"
        android:icon="@drawable/game_over"
        android:label="@string/app_name"
        android:roundIcon="@drawable/game_over"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".GameOverActivity" />
        <activity android:name=".MultiplayerActivity" />
        <activity android:name=".MultiplayerGameActivity" />
        <activity android:name=".ScoreActivity"></activity>
    </application>

</manifest>